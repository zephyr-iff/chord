{
  "name": "chordpic",
  "description": "create chord charts with ease",
  "version": "1.0.0",
  "private": true,
  "author": "Raphael Voellmy <r.voellmy@gmail.com>",
  "keywords": [
    "gatsby",
    "guitar",
    "chords"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build:ci": "run-s test build",
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "run-p dev",
    "test": "run-s type-check test:jest",
    "test:jest": "jest",
    "type-check": "tsc"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
